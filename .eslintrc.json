{
  "extends": "eslint-config-eyeo",
  "root": true,
  "rules": {
    "id-blacklist": [
      "error",
      "blacklist", "blackList",
      "_blacklist", "_blackList",
      "Blacklist", "BlackList",
      "BLACKLIST", "BLACK_LIST"
    ],
    "nonblock-statement-body-position": ["error", "below"]
  }
}
